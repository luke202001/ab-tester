{
  "stack": "nodejs",
  "memory": 128,
  "routes": {
    "timeToCompleteAbTest": {
      "path": "/_v/private/abtesting/time/:probability",
      "public": false
    },
    "initializeAbTestForWorkspace": {
      "path": "/_v/private/abtesting/initialize/:workspace/:probability",
      "public": false
    },
    "abTestStatus": {
      "path": "/_v/private/abtesting/status",
      "public": false
    },
    "finishAbTestForWorkspace": {
      "path": "/_v/private/abtesting/finish/:workspace",
      "public": false
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": ["linked"]
    }
  }
}